{
	"name": "packme",
	"version": "2.0.0",
	"description": "Blazing fast binary serialization via auto-generated classes from simple JSON manifest files.",
	"type": "module",
	"module": "dist/packme.min.js",
	"bin": {
		"packme": "bin/compile.js"
	},
